{
  "name": "SemSurf",
  "background_page": "listener.html",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "semsurf",
      "default_icon": "icon.png",
       "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "matches": ["http://*/*", "https://*/*"], 
    "js": ["executor.js"]
  }]
}